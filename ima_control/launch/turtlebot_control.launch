<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find ima_control)/config/turtlebot_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/turtlebot" args="wheel_left_effort_controller wheel_right_effort_controller joint_state_controller"/>

</launch>
